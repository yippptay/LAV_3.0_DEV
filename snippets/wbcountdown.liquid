{% if end_date != blank %}
<div class="timer_outer">
    <div class="row">
        <div class="col-sm-5 col-12 text-start ptimer">
            {% if title != blank %}<h5>{{ title }}</h5>{% endif %}
        </div>
        <div class="col-sm-7 col-12">
            <div class="countdowntimer">      
                <div data-date="{{end_date}}"></div>
                <div data-countdown="{{end_date}}" class="wbcounttimer">
                </div>
            </div>
        </div>
    </div>
</div>
{% endif %} 

{%comment%}<div class="countdowntimer wbcounttimer"><div><span>369</span><strong>Days</strong></div><div><span>11</span><strong>Hours</strong></div><div><span>34</span><strong>Min</strong></div><div><span>32</span><strong>Sec</strong></div></div>{%endcomment%}   